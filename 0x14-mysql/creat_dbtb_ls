mysql> CREATE DATABASE IF NOT EXISTS tyrell_corp; 
mysql> CREATE TABLE IF NOT EXISTS tyrell_corp.nexus6 (id INT UNIQUE AUTO_INCREMENT NOT NULL, name VARCHAR(256) NOT NULL, PRIMARY KEY (id));                                             
mysql> SHOW databases;
mysql> SHOW tables;
mysql> use tyrell_corp;
mysql> INSERT INTO `nexus6` (`id`, `name`) VALUES (1, "Leon"); 
mysql> SELECT * FROM nexus6;
mysql> GRANT SELECT ON tyrell_corp.* TO 'holberton_user'@'localhost';                    
